{
  "rules": {
    "align": [true, "elements", "members", "parameters", "statements"],
    "import-name": false,
    "variable-name": false,
    "max-line-length": [true, 144],
    "no-increment-decrement": false,
    "no-consecutive-blank-lines": [true, 2],
    "no-console": true,
    "strict-boolean-expressions": false,
    "prefer-object-spread": true
  },
  "linterOptions": {
    "exclude": [
    ]
  }
}
